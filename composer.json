{
    "name": "phpieribrahim/laravel-dompdf-ar",
    "description": "A DOMPDF Wrapper for Laravel with AR support",
    "license": "MIT",
    "keywords": ["laravel", "dompdf", "pdf", "laravel-dompdf-ar", "dompdf-ar", "ar"],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
{
            "name": "Ibrahim Abdel Wahab",
            "email": "phpieribrahim@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.4.x|^5.5|^6|^7|^8",
        "phpieribrahim/laravel-dompdf-ar": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "phpieribrahim\\DomPDF\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        },
        "laravel": {
            "providers": [
                "phpieribrahim\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "phpieribrahim\\DomPDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
